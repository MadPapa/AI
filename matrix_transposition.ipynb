    "import numpy as np\n",
    "\n",
    "def matrix_transposition(array_1):\n",
    "        return np.asmatrix([[array_1[j][i] for j in range(len(array_1))] for i in range(len(array_1[0]))])\n",
    "\n",
    "\n",
    "matrix = [\n",
    "    [1, 2, 3],\n",
    "    [4, 5, 6],\n",
    "    [7, 8, 9]\n",
    "]\n",
    "\n",
    "matrix_transposition(matrix)"
